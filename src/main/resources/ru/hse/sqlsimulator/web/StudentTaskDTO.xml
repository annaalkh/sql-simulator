<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="ru.hse.sqlsimulator.web.dto">
  <class name="StudentTaskDTO" table="tasks">
    <id column="id" name="id" type="int">
      <generator class="increment"/>
    </id>
    <property column="name" name="name"/>
    <property column="description" name="description"/>
    <property column="is_active" name="is_active"/>
    <property column="is_planned" name="is_planned"/>
    <property column="start_date" name="start_date"/>
    <property column="end_date" name="end_date"/>
    <property column="act_date" name="act_date"/>
  </class>
</hibernate-mapping>
